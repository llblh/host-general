{
  "name": "@carpenter/host-general",
  "version": "0.0.1",
  "description": "移动端通用",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "bash deploy.sh",
    "lint": "vue-cli-service lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/llblh/host-general.git"
  },
  "main": "./dist/@carpenter/host-general.umd.min.js",
  "dependencies": {
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "vue": "^2.6.6",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
